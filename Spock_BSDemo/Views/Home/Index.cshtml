                                            <div class="col-md-12" id="smallest"></div>
                                        </div>
                                        <div class="row mb-3 h4">
                                            <div class="col-md-12" id="largest"></div>
                                        </div>
                                        <div class="row mb-3 h4">
                                            <div class="col-md-12" id="sum"></div>
                                        </div>
                                        <div class="row mb-3 h4">
                                            <div class="col-md-12" id="product"></div>
                                        </div>
                                        <div class="row mb-3 h4">
                                            <div class="col-md-12" id="average"></div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="d-inline-block ml-2">
                                            <button id="btnStartMath" type="button" class="btn">Start</button>
                                            <button id="btnToggleCodeMath" type="button" class="btn btn-light">Show Code</button>
                                            <button id="btnClearMath" type="button" class="btn">Clear</button>
                                        </div>
                                    </div>
