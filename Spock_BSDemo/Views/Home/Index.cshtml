                                            <pre class="brush:js">
                    
                                                $("#btnPal").click(function () {

                                                    var word = $("#palIn").val();
                                                    var revWord = "";
           
                                                    for (var i = word.length - 1; i >= 0; i--) {
                                                        revWord += word.substr(i, 1);
                                                    }

                                                    if (revWord == word) {
                                                        $("#palOut").text("This is Palindrome:" + ' ' + revWord);
                                                    } else {
                                                        $("#palOut").text("This is not Palindrome:" + ' ' + revWord);
                                                    }
                                                });

